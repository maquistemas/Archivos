       IF i_reporte = 'RCA' THEN
          IF i_tipo_informacion IS NULL    OR
             i_tipo_informacion NOT IN ('4', '5') THEN
             li_resultado:= -1;
          END IF;
       ELSIF i_reporte = 'RCM' THEN
          IF i_tipo_informacion IS NULL    OR
