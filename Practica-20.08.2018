      END IF;
   END IF;

   IF i_tip_persona IN ('1', '3') THEN
      IF i_tip_doc_iden = '1' AND LENGTH(i_num_doc_iden) <> 8 THEN
         li_resultado  := -1;
      END IF;
   END IF;
--  END IF;

  Return li_resultado;

EXCEPTION
   WHEN OTHERS THEN
        RETURN -1;
END SF_VALIDA_CONTROL_69;

