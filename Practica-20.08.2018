      END IF;
   END IF;

-- Actualiza CRA_PAR_PROCESOS
   SP_REG_CONTROL (i_anopro, i_mespro, i_codemp, n_cod_ide_pro, n_cod_est_pro, NULL, i_dia_refer, i_cod_sec_envio);
-- Actualiza CRA_CONTROL_PROCESOS
