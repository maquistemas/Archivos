-- --------------------------------------------------------------
-- Autor   : Jaime Lopez-Torres M. - Enero 2004
-- Objetivo: Borra registros de un control en tabla CRA_VAL_CONTROLES_X_EMPRESA
-- --------------------------------------------------------------
IS
BEGIN
