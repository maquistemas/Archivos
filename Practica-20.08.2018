            n_control := 98;
            IF arr_activ (n_control) = 'S'  THEN
               n_rc := 0;
               -- Se reutiliza la validación del control 60, que tiene la misma lógica que se debe aplicar para este control
               n_rc := sf_valida_control_60 ( 0, '2', ls_razon_social_adq, n_reporte );
