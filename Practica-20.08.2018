PROCEDURE SP_BORRA_VALID_DEV_IDENTIF_RTC
         (i_anopro  IN VARCHAR2,
          i_mespro  IN VARCHAR2,
          i_codemp  IN VARCHAR2)
-- -----------------------------------------------------------------------------
-- Autor   : Jaime Lopez-Torres M. - Enero 2003
-- Objetivo: Borra registros de tabla CRA_VALID_DEV_IDENTIF_RTC.
-- -----------------------------------------------------------------------------
IS
BEGIN
   n_cont := 0;
   n_cod_ide_pro := 439;
