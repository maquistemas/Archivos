                              arr_error (n_control) := arr_error (n_control) + 1;
                              SP_VAL_GRABA_VALID_ERRORES (i_anopro, i_mespro, i_codemp, n_control, cur.num_sec_reg, 0, 'VA', arr_tipcon(n_control), n_reporte );
                           END IF;
                        END IF;
                     END IF;
                  END IF;
               END IF;
             EXCEPTION
               WHEN OTHERS THEN
                    n_no_encuentra := 1;
             END;
          END IF;
       END IF;

